    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/puppeteer": "^3.0.1",
    "husky": "^4.2.5",
    "puppeteer": "^5.2.1",
    "ts-node": "^9.0.0",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
